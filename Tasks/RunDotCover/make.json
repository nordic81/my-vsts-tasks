{
    "externals": {
        "nugetv2": [
            {
                "name": "VstsTaskSdk",
                "version": "0.10.0",
                "repository": "https://www.powershellgallery.com/api/v2/",
                "cp": [
                    {
                        "source": [
                            "*.dll",
                            "*.ps1",
                            "*.psd1",
                            "*.psm1",
                            "lib.json",
                            "Strings"
                        ],
                        "dest": "ps_modules/VstsTaskSdk/",
                        "options": "-R"
                    }
                ]
            },
            {
                "name": "JetBrains.dotCover.CommandLineTools",
                "version": "2019.3.1",
                "repository": "https://www.nuget.org/api/v2/",
                "cp": [
                    {
                        "source": [
                            "tools/*.*"
                        ],
                        "dest": "tools/dotCover/",
                        "options": "-R"
                    },
                    {
                        "source": [
                            "tools/x86/*.*"
                        ],
                        "dest": "tools/dotCover/x86",
                        "options": "-R"
                    },
                    {
                        "source": [
                            "tools/x64/*.*"
                        ],
                        "dest": "tools/dotCover/x64",
                        "options": "-R"
                    }
                ]
            },
            {
                "name": "ReportGenerator",
                "version": "4.4.6.0",
                "repository": "https://www.nuget.org/api/v2/",
                "cp": [
                    {
                        "source": [
                            "tools/*.*"
                        ],
                        "dest": "tools/ReportGenerator/",
                        "options": "-R"
                    }
                ]
            }
        ]
    }
}
